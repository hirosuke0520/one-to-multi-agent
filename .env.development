# Development Environment Variables
NODE_ENV=development

# Database (localhost for testing without Docker)
DB_HOST=localhost
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=one_to_multi_agent
DB_PORT=5432

# Storage (Local for development)
STORAGE_TYPE=local
DATA_DIR=./data
TEMP_DIR=./temp
STORAGE_DIR=./storage

# AI (Disabled for development testing)
USE_REAL_AI=false
GOOGLE_API_KEY=dummy_key_for_development

# API Server
PORT=8787

# Web Frontend
NEXT_PUBLIC_API_URL=http://localhost:8787

# Auth (Development placeholders)
AUTH_GOOGLE_ID=development_google_id
AUTH_GOOGLE_SECRET=development_google_secret
AUTH_URL=http://localhost:3000
AUTH_SECRET=development_secret_12345