# Production Environment Variables for GCP

# Database (Cloud SQL)
DB_HOST=/cloudsql/one-to-multi-agent:us-central1:one-to-multi-agent-db
DB_USER=postgres
DB_PASSWORD=YOUR_DB_PASSWORD_HERE
DB_NAME=one_to_multi_agent
DB_PORT=5432

# GCP Configuration
GCP_PROJECT_ID=one-to-multi-agent
GCS_BUCKET_NAME=one-to-multi-agent-storage

# Google AI (Gemini)
GOOGLE_API_KEY=YOUR_GEMINI_API_KEY_HERE
USE_REAL_AI=true

# Application
NODE_ENV=production
PORT=8080

# API URLs (for frontend)
NEXT_PUBLIC_API_URL=https://api-YOUR_CLOUD_RUN_URL.run.app

# File Storage
STORAGE_TYPE=gcs
TEMP_DIR=/app/temp